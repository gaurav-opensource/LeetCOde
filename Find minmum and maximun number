       int max = Integer.MIN_VALUE;
       int min = Integer.MAX_VALUE;


       //print the numbers in array
       for(int i=0; i<numbers.length; i++) {
         if(numbers[i]>max){
            max = numbers[i];
         }else{
            if(numbers[i]<min){
                min = numbers[i];
         } 
       }
   }
   System.out.println(max + " " +min);